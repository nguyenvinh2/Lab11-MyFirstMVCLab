@model List<MyFirstMVCApp.Models.Person>


  <head>
    <link href="~/StyleSheet.css" rel="stylesheet"/>
  </head>

  <body>
    <h2>Your Search Yields the Following:</h2>
    <table>
      <tr>
        <th>Year</th>
        <th>Honor</th>
        <th>Name</th>
        <th>Country</th>
        <th>Birth</th>
        <th>Death</th>
        <th>Title</th>
        <th>Category</th>
        <th>Context</th>
      </tr>
      @foreach (var item in Model)
      {
        <tr>
          <td>@item.Year</td>
          <td>@item.Honor</td>
          <td>@item.Name</td>
          <td>@item.Country</td>
          <td>@item.BirthYear</td>
          <td>@item.DeathYear</td>
          <td>@item.Title</td>
          <td>@item.Category</td>
          <td>@item.Context</td>
        </tr>
      }
    </table>
  </body>